{
  "name": "Theme displayer",
  "description": "Gets the current pages theme",
  "version": "1.0",
  "manifest_version" : 2,
  "permissions" : [
    "activeTab"
  ], 
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Get content background"
  }
}